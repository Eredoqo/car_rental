{
  "compilerOptions": {
    "lib": ["ES2023"],
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./build",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["./prisma/**"]
}
